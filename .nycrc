{
    "check-coverage": false,
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "include": [
        "test/**/*.ts",
        "src/**/*.ts"
    ],
    "exclude": [
      "**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "ts-node/register"
    ],
    "extension": [
      ".ts"
    ],
    "cache": false,
    "all": false,
    "sourceMap": true,
    "instrument": true,
    "report-dir": "./coverage"
}
