{
  "name": "javascript-dependency-injection",
  "version": "2.0.0-rc.1",
  "description": "Javscript dependency injection library",
  "main": "dist/di.js",
  "scripts": {
    "build": "./node_modules/.bin/babel di.js --out-dir dist",
    "doc": "./node_modules/.bin/esdoc -c esdoc.json && open esdoc/index.html",
    "test": "./node_modules/.bin/babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha -- --recursive",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scaljeri/javascript-dependency-injection.git"
  },
  "keywords": [
    "javascript",
    "dependency",
    "injection",
    "di"
  ],
  "author": "Lucas Calje <lucas@calje.eu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scaljeri/javascript-dependency-injection/issues"
  },
  "homepage": "https://github.com/scaljeri/javascript-dependency-injection",
  "devDependencies": {
    "babel-cli": "6.24.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-stage-0": "6.24.0",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "esdoc": "0.5.2",
    "esdoc-es7-plugin": "0.0.3",
    "isparta": "4.0.0",
    "mocha": "3.2.0",
    "sinon": "2.1.0",
    "sinon-chai": "2.9.0"
  },
  "dependencies": {
    "babelify": "^7.3.0",
    "browserify": "^14.3.0"
  }
}
