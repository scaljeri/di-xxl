{
  "name": "di-xxl",
  "version": "1.0.0-rc.2",
  "description": "Javscript dependency injection library",
  "main": "dist/di.js",
  "scripts": {
    "build": "./node_modules/.bin/babel di.js --out-dir dist && ./node_modules/.bin/uglifyjs dist/di.js -o dist/di.min.js",
    "doc": "./node_modules/.bin/jsdoc di.js -c jsdoc.json --destination docs && open docs/index.html",
    "prepublish": "npm run build",
    "test": "cross-env NODE_ENV=test nyc mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scaljeri/javascript-dependency-injection.git"
  },
  "keywords": [
    "javascript",
    "dependency",
    "injection",
    "di",
    "es2015"
  ],
  "author": "Lucas Calje <lucas@calje.eu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scaljeri/di-xx/issues"
  },
  "homepage": "https://github.com/scaljeri/di-xxl",
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.0.2",
    "chai-as-promised": "^7.1.1",
    "esdoc": "1.0.1",
    "esdoc-es7-plugin": "0.0.3",
    "isparta": "4.0.0",
    "mocha": "^3.4.2",
    "sinon": "^2.3.6",
    "sinon-chai": "^2.11.0"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-remove-comments": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-stage-0": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "chai-arrays": "^1.0.0",
    "cross-env": "^5.0.1",
    "esdoc-standard-plugin": "^1.0.0",
    "jsdoc": "^3.5.4",
    "jsdoc-escape-at": "^1.0.1",
    "nyc": "^11.0.3",
    "uglify-js": "^3.0.27"
  }
}
