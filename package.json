{
  "name": "javascript-dependency-injection",
  "version": "2.0.0-rc.1",
  "description": "Javscript dependency injection library",
  "main": "dist/di.js",
  "scripts": {
    "build": "./node_modules/.bin/babel di.js --out-dir dist",
    "doc": "./node_modules/.bin/esdoc -c esdoc.json && open esdoc/index.html",
    "old-test": "./node_modules/.bin/babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha -- --recursive",
    "prepublish": "npm run build",
    "test": "cross-env NODE_ENV=test nyc mocha",
    "t1": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha test/*.js --compilers js:babel-core/register",
    "t2": "./node_modules/.bin/babel-node ./node_modules/.bin/mocha --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scaljeri/javascript-dependency-injection.git"
  },
  "keywords": [
    "javascript",
    "dependency",
    "injection",
    "di"
  ],
  "author": "Lucas Calje <lucas@calje.eu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scaljeri/javascript-dependency-injection/issues"
  },
  "homepage": "https://github.com/scaljeri/javascript-dependency-injection",
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.0.2",
    "chai-as-promised": "^7.1.1",
    "esdoc": "1.0.1",
    "esdoc-es7-plugin": "0.0.3",
    "isparta": "4.0.0",
    "mocha": "^3.4.2",
    "sinon": "^2.3.6",
    "sinon-chai": "^2.11.0"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-stage-0": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "chai-arrays": "^1.0.0",
    "cross-env": "^5.0.1",
    "nyc": "^11.0.3"
  }
}
